{
  "resourceType": "StructureDefinition",
  "id": "JP-Encounter-eClinicalSummary",
  "extension": [
    {
      "url": "http://hl7.org/fhir/StructureDefinition/structuredefinition-category",
      "valueString": "Base.Management"
    },
    {
      "url": "http://hl7.org/fhir/StructureDefinition/structuredefinition-security-category",
      "valueCode": "patient"
    }
  ],
  "url": "http://jpfhir.jp/fhir/eClinicalSummary/StructureDefinition/JP_Encounter_eClinicalSummary",
  "version": "0.9.1",
  "name": "JP_Encounter_eClinicalSummary",
  "status": "active",
  "date": "2022-11-07",
  "description": "診療情報提供では、紹介理由を記述するEncouter情報　JP_Encounterの派生プロファイル。診療サマリーではサマリー対象となる受診や入院に関する情報。",
  "fhirVersion": "4.0.1",
  "mapping": [
    {
      "identity": "workflow",
      "uri": "http://hl7.org/fhir/workflow",
      "name": "Workflow Pattern"
    },
    {
      "identity": "rim",
      "uri": "http://hl7.org/v3",
      "name": "RIM Mapping"
    },
    {
      "identity": "w5",
      "uri": "http://hl7.org/fhir/fivews",
      "name": "FiveWs Pattern Mapping"
    },
    {
      "identity": "v2",
      "uri": "http://hl7.org/v2",
      "name": "HL7 v2 Mapping"
    }
  ],
  "kind": "resource",
  "abstract": false,
  "type": "Encounter",
  "baseDefinition": "http://jpfhir.jp/fhir/core/StructureDefinition/JP_Encounter",
  "derivation": "constraint",
  "differential": {
    "element": [
      {
        "id": "Encounter.status",
        "path": "Encounter.status",
        "definition": "finished の固定値を設定する。",
        "fixedCode": "finished",
        "mustSupport": true
      },
      {
        "id": "Encounter.class",
        "path": "Encounter.class",
        "definition": "外来（外来）、入院、救急、在宅医療、その他の地域差による患者Encouterの分類を表す概念。",
        "mustSupport": true
      },
      {
        "id": "Encounter.class.system",
        "path": "Encounter.class.system",
        "definition": "コード体系v3.ActEncounterCodeを識別するURI。固定値。",
        "min": 1,
        "fixedUri": "http://terminology.hl7.org/CodeSystem/v3-ActCode",
        "mustSupport": true
      },
      {
        "id": "Encounter.class.code",
        "path": "Encounter.class.code",
        "definition": "AMB:外来　EMER:救急　HH:在宅ケア    IMP:入院または入院中    ACUTE: 入院中臨時     NONAC:入院中定時   PRENC:予定入院時    VR:リモート診療",
        "min": 1,
        "mustSupport": true,
        "binding": {
          "strength": "extensible",
          "valueSet": "http://terminology.hl7.org/ValueSet/v3-ActEncounterCode"
        }
      },
      {
        "id": "Encounter.class.display",
        "path": "Encounter.class.display",
        "definition": "AMB:外来　EMER:救急　HH:在宅ケア    IMP:入院または入院中    ACUTE: 入院中臨時     NONAC:入院中定時   PRENC:予定入院時    VR:リモート診療",
        "mustSupport": true
      },
      {
        "id": "Encounter.classHistory",
        "path": "Encounter.classHistory",
        "max": "1"
      },
      {
        "id": "Encounter.reasonCode",
        "path": "Encounter.reasonCode",
        "short": "入院時主訴・入院理由。紹介する理由（主訴・目的）",
        "definition": "コードで記述できる場合にそのコード記述。system値はMEDIS標準病名マスター病名交換用コードを使用できる。text要素にフリーテキストで記述してもよい。",
        "mustSupport": true
      },
      {
        "id": "Encounter.reasonCode.coding.system",
        "path": "Encounter.reasonCode.coding.system",
        "mustSupport": true
      },
      {
        "id": "Encounter.reasonCode.coding.code",
        "path": "Encounter.reasonCode.coding.code",
        "mustSupport": true
      },
      {
        "id": "Encounter.reasonCode.text",
        "path": "Encounter.reasonCode.text",
        "mustSupport": true
      }
    ]
  }
}
